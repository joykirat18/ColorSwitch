<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="700.0" prefWidth="500.0" fx:controller="main.controllers.LandingController">
    <GridPane prefHeight="377.0" prefWidth="500.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="120.0" minHeight="7.0" prefHeight="85.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="245.0" minHeight="10.0" prefHeight="68.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="245.0" minHeight="10.0" prefHeight="66.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="286.0" minHeight="10.0" prefHeight="158.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <VBox alignment="CENTER" GridPane.rowIndex="1">
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="199.0"
                    style="-fx-background-radius: 50;" text="Login" fx:id="loginButton">
                <styleClass>
                    <String fx:value="button"/>
                    <String fx:value="login_button"/>
                </styleClass>
            </Button>
        </VBox>
        <VBox alignment="CENTER" layoutX="10.0" layoutY="97.0" prefHeight="68.0" prefWidth="500.0"
              GridPane.rowIndex="2">
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0"
                    style="-fx-background-radius: 50;" text="Signup" fx:id="signUpButton">
                <styleClass>
                    <String fx:value="button"/>
                    <String fx:value="signup_button"/>
                </styleClass>
            </Button>
        </VBox>
    </GridPane>
</AnchorPane>
