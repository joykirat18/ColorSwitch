<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<AnchorPane xmlns:fx="http://javafx.com/fxml"
            xmlns="http://javafx.com/javafx"
            prefHeight="700.0" prefWidth="500.0" fx:controller="main.controllers.LandingController">
    <GridPane prefHeight="377.0" prefWidth="500.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="120.0" minHeight="7.0" prefHeight="85.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="245.0" minHeight="10.0" prefHeight="68.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="245.0" minHeight="10.0" prefHeight="66.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="286.0" minHeight="10.0" prefHeight="158.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <VBox alignment="CENTER" GridPane.rowIndex="1">
            <Button onMouseClicked="#onLoginClick" mnemonicParsing="false" prefHeight="40.0" prefWidth="199.0"
                    text="Login" fx:id="loginButton">
                <styleClass>
                    <String fx:value="login-button"/>
                </styleClass>
            </Button>
        </VBox>
        <VBox alignment="CENTER" layoutX="10.0" layoutY="97.0" prefHeight="68.0" prefWidth="500.0"
              GridPane.rowIndex="2">
            <Button fx:id="signUpButton" onMouseClicked="#onSignUpClick" mnemonicParsing="false" prefHeight="40.0"
                    prefWidth="200.0"
                    text="Signup">
                <styleClass>
                    <String fx:value="signup-button"/>
                </styleClass>
            </Button>
        </VBox>
    </GridPane>
</AnchorPane>
