<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="700.0" prefWidth="500.0"
            stylesheets="@../styles/MainLayout.css" xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="main.controllers.LoadGameController">
    <ScrollPane fx:id="scrollPane" prefHeight="360" prefWidth="500.0" styleClass="background-dark"
                stylesheets="@../styles/MainLayout.css">
        <VBox fx:id="buttonContainer" alignment="CENTER" prefHeight="360" prefWidth="482.0" spacing="20.0"
              styleClass="background-dark" stylesheets="@../styles/MainLayout.css">
            <padding>
                <Insets bottom="20.0" top="20.0"/>
            </padding>
            <Button fx:id="loadGameButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                    onMouseClicked="#onLoadGameClick" prefHeight="40.0" prefWidth="200.0" text="Load Game 1"
                    textAlignment="CENTER"/>
        </VBox>
    </ScrollPane>
</AnchorPane>
