<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<fx:root type="AnchorPane" prefHeight="700.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1"
         xmlns:fx="http://javafx.com/fxml/1" stylesheets="@../styles/MainLayout.css">
    <GridPane prefHeight="700.0" prefWidth="500.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0" GridPane.rowIndex="0">
            <fx:include fx:id="heading" source="Heading.fxml"/>
        </AnchorPane>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0" GridPane.rowIndex="1">
            <fx:include fx:id="loadGame" source="LoadGame.fxml"/>
            <fx:include fx:id="mainMenu" source="MainMenu.fxml"/>
            <fx:include fx:id="login" source="Login.fxml"/>
            <fx:include fx:id="landing" source="Landing.fxml"/>
        </AnchorPane>
    </GridPane>
</fx:root>
