<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.SVGPath?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane fx:id="rootPane" prefHeight="700.0" prefWidth="500.0" stylesheets="@../styles/MainLayout.css"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="main.controllers.StartGameController">
    <!--   <stylesheets>-->
    <!--      <String fx:value="@../styles/Game.css"/>-->
    <!--   </stylesheets>-->
    <!--   <fx:include source="PausePopUp.fxml" fx:id="popUpContainer"/>-->
    <GridPane layoutX="2.0" layoutY="-2.0" prefHeight="700.0" prefWidth="502.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="158.0" minWidth="10.0" prefWidth="95.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="314.0162658691406" minWidth="10.0"
                               prefWidth="282.6720886230469"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="140.0" minWidth="10.0" prefWidth="124.32791137695312"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="226.0" minHeight="0.0" prefHeight="85.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="615.0" minHeight="10.0" prefHeight="615.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <AnchorPane prefHeight="200.0" prefWidth="200.0">
            <VBox alignment="CENTER" prefHeight="91.0" prefWidth="84.0">
                <Button fx:id="pauseButton" mnemonicParsing="false" onMouseClicked="#onPauseClick"
                        styleClass="bg-transparent">
                    <graphic>
                        <SVGPath
                                content="M8 19c1.1 0 2-.9 2-2V7c0-1.1-.9-2-2-2s-2 .9-2 2v10c0 1.1.9 2 2 2zm6-12v10c0 1.1.9 2 2 2s2-.9 2-2V7c0-1.1-.9-2-2-2s-2 .9-2 2z"
                                fill="WHITE" scaleX="2.0" scaleY="2.0" stroke="WHITE" strokeWidth="0.0"/>
                    </graphic>
                    <font>
                        <Font size="25.0"/>
                    </font>
                </Button>
            </VBox>
        </AnchorPane>
        <AnchorPane prefHeight="75.0" prefWidth="108.0" GridPane.columnIndex="2">
            <HBox alignment="CENTER" layoutX="-3.0" layoutY="15.0" prefHeight="93.0" prefWidth="135.0">
                <ImageView fitHeight="35.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                    <Image url="@../images/Star.png"/>
                </ImageView>
                <Text fx:id="scoreText" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="989"
                      textAlignment="CENTER" wrappingWidth="53.226959228515625">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Text>
            </HBox>
        </AnchorPane>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="1">
            <Button fx:id="endButton" mnemonicParsing="false" onMouseClicked="#onEndClick" prefHeight="29.0"
                    prefWidth="81.0" text="EndGame">
                <VBox.margin>
                    <Insets bottom="10.0"/>
                </VBox.margin>
            </Button>
        </VBox>
        <VBox alignment="BOTTOM_CENTER" prefHeight="615.0" prefWidth="258.0" GridPane.columnIndex="1"
              GridPane.rowIndex="1">
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
            <VBox prefHeight="243.0" prefWidth="283.0"/>
            <Button alignment="BOTTOM_CENTER" mnemonicParsing="false" onMouseClicked="#onJumpClick" text="Jump"
                    textAlignment="CENTER">
                <VBox.margin>
                    <Insets bottom="10.0"/>
                </VBox.margin>
            </Button>
        </VBox>
    </GridPane>
</AnchorPane>
