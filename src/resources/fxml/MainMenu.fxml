        </VBox>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="1">
            <Button fx:id="newGameButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0"
